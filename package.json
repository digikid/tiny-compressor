    "@typescript-eslint/parser": "^5.30.4",
    "chalk": "^5.0.1",
    "cli-spinners": "^2.6.1",
    "command-line-usage": "^6.1.3",
    "configstore": "^6.0.0",
    "dree": "^3.3.7",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "file-type": "^17.1.2",
    "inquirer": "^9.0.0",
    "log-symbols": "^5.1.0",
