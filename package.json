{
  "name": "tiny-compressor",
  "version": "1.2.2",
  "description": "A command line JPG, PNG and WebP images compressor using TinyPNG API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tiny-compressor": "dist/index.js"
  },
  "homepage": "https://github.com/digikid/tiny-compressor#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/digikid/tiny-compressor.git"
  },
  "bugs": {
    "url": "https://github.com/digikid/tiny-compressor/issues"
  },
  "keywords": [
    "compress",
    "image",
    "jpeg",
    "jpg",
    "png",
    "webp",
    "tinypng"
